				{
					"name": "Get Residence by Code",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 200\", function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"pm.test(\"Response has residence data\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData.data).to.be.an('object');",
									"    pm.expect(jsonData.data.residence_name).to.exist;",
									"});",
									"",
									"// Save residence data",
									"var jsonData = pm.response.json();",
									"pm.environment.set(\"residence_name\", jsonData.data.residence_name);"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "GET",
						"header": [
							{
								"key": "Accept",
								"value": "application/json",
								"type": "text"
							}
						],
						"url": {
							"raw": "{{base_url}}/residence-by-code/{{registration_uuid}}/",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"residence-by-code",
								"{{registration_uuid}}",
								""
							]
						},
						"description": "Retrieve residence information using registration code (UUID)"
					},
					"response": []
				},
				{
					"name": "Get Units by Code",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 200\", function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"pm.test(\"Response has units array\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData.data).to.be.an('array');",
									"    pm.expect(jsonData.data.length).to.be.above(0);",
									"});",
									"",
									"// Save first unit ID for registration",
									"var jsonData = pm.response.json();",
									"if (jsonData.data && jsonData.data.length > 0) {",
									"    pm.environment.set(\"unit_id\", jsonData.data[0].unit_id);",
									"    pm.environment.set(\"unit_name\", jsonData.data[0].unit_name);",
									"}"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "GET",
						"header": [
							{
								"key": "Accept",
								"value": "application/json",
								"type": "text"
							}
						],
						"url": {
							"raw": "{{base_url}}/units/code/{{registration_uuid}}/",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"units",
								"code",
								"{{registration_uuid}}",
								""
							]
						},
						"description": "Get list of available units in the residence"
					},
					"response": []
				}
			],
			"description": "Endpoints for loading initial data before registration form"
		},
