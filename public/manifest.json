    "images": [
        {
            "src": "https://pewaca.id/assets/images/icon-192x192.png",
            "sizes": "192x192",
            "type": "image/png",
            "purpose": "any"
        },
        {
            "src": "https://pewaca.id/assets/images/icon-512x512.png",
            "sizes": "512x512",
            "type": "image/png",
            "purpose": "maskable"
        }
    ],
    "screenshots": [
        {
            "src": "https://pewaca.id/assets/screenshots/home-desktop.png",
            "sizes": "1080x1920",
            "type": "image/png"
        },
        {
            "src": "https://pewaca.id/assets/screenshots/home-mobile.png",
            "sizes": "1080x1920",
            "type": "image/png"
        }
    ],
    "shortcuts": [
        {
            "name": "Open Articles",
            "short_name": "Articles",
